# Example Environment Variables
# Copy this to .env.local and update the values

# App Configuration
NEXT_PUBLIC_APP_NAME="Lung Disease Diagnosis AI"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_APP_DESCRIPTION="AI-powered lung disease diagnosis from X-ray images"

# API Configuration
NEXT_PUBLIC_API_URL="http://localhost:3000"
NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# Model Configuration
NEXT_PUBLIC_MODEL_URL="/models/lung-disease-model.json"
NEXT_PUBLIC_MODEL_VERSION="1.0.0"

# Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE="10485760"  # 10MB in bytes
NEXT_PUBLIC_ALLOWED_FILE_TYPES="image/jpeg,image/jpg,image/png,image/webp"

# Analytics (optional)
NEXT_PUBLIC_GA_ID=""
NEXT_PUBLIC_ANALYTICS_ENABLED="false"

# Feature Flags
NEXT_PUBLIC_ENABLE_GRADCAM="true"
NEXT_PUBLIC_ENABLE_CONFIDENCE_THRESHOLD="true"
NEXT_PUBLIC_CONFIDENCE_THRESHOLD="0.5"

# Contact Information
NEXT_PUBLIC_CONTACT_EMAIL="support@lungdiagnosis.com"
NEXT_PUBLIC_GITHUB_URL="https://github.com/your-username/lung-disease-diagnosis"

# Security
NEXT_PUBLIC_RATE_LIMIT_REQUESTS="10"
NEXT_PUBLIC_RATE_LIMIT_WINDOW="900000"  # 15 minutes in milliseconds

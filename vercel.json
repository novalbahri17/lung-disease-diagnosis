{
  "version": 2,
  "name": "ai-diagnosis-paru-paru",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "pages/api/predict.js": {
      "maxDuration": 30
    },
    "pages/api/health.js": {
      "maxDuration": 10
    }
  }
}
